                <xs:element xmlns:plc="http://www.teslanet.nl/schema/mule/plc" minOccurs="0" maxOccurs="unbounded" ref="plc:abstract-unsubscribe-field"></xs:element>
              </xs:sequence>
            </xs:complexType>
